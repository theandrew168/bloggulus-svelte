{
	"name": "bloggulus",
	"version": "0.0.1",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "prisma generate && vite build",
		"start": "prisma migrate deploy && node ./build/",
		"generate": "prisma generate",
		"migrate": "prisma migrate deploy",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"format": "prettier --write ."
	},
	"dependencies": {
		"@prisma/client": "^5.7.1",
		"rss-parser": "^3.13.0"
	},
	"devDependencies": {
		"@flydotio/dockerfile": "^0.4.11",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.30.3",
		"@types/node": "^20.10.5",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prisma": "^5.7.1",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"tslib": "^2.6.2",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3",
		"vite": "^4.5.1"
	},
	"prettier": {
		"arrowParens": "always",
		"bracketSameLine": false,
		"bracketSpacing": true,
		"jsxSingleQuote": false,
		"printWidth": 120,
		"semi": true,
		"singleQuote": false,
		"trailingComma": "all",
		"useTabs": true,
		"plugins": [
			"prettier-plugin-svelte"
		],
		"overrides": [
			{
				"files": "*.svelte",
				"options": {
					"parser": "svelte"
				}
			}
		]
	}
}
